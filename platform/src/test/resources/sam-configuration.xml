<services>
    <service name="lastfm"
             handler="tv.notube.usermanager.services.auth.lastfm.LastFmAuthHandler">
        <description>Provides access to Lastfm user data</description>
        <apikey>YOUR_API_KEY</apikey>
        <secret>YOUR_API_SECRET</secret>
        <session>http://ws.audioscrobbler.com/2.0/?</session>
        <endpoint>http://ws.audioscrobbler.com/2.0/?</endpoint>
    </service>
    <service name="twitter"
             handler="tv.notube.usermanager.services.auth.twitter.TwitterAuthHandler">
        <description>Provides access to Twitter user data</description>
        <apikey>YOUR_API_KEY</apikey>
        <secret>YOUR_API_SECRET</secret>
        <session>https://api.twitter.com/oauth/request_token</session>
        <endpoint>https://api.twitter.com/1/statuses/user_timeline.json
        </endpoint>
    </service>
    <service name="facebook"
             handler="tv.notube.usermanager.services.auth.facebook.FacebookAuthHandler">
        <description>Provides access to Facebook user data</description>
        <apikey>YOUR_API_KEY</apikey>
        <secret>YOUR_API_SECRET</secret>
        <session></session>
        <endpoint>https://graph.facebook.com/me/likes</endpoint>
    </service>
</services>